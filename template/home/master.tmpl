<script type="text/javascript">
    $('#intro').find('img').each(function(){
        var alt = this.alt;
        if (alt){
            $(this).after('<span class="caption" style="display:none">' + alt + '</span>');
        }
        $(this).wrap('<a href="' + this.src + '" title="' + alt + '" class="fancybox" rel="gallery" />');
    });
    (function($){
        $('.fancybox').fancybox();
    })(jQuery);
</script>



